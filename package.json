{
    "name": "vue-flickr-places",
    "version": "0.1.0",
    "private": true,
    "description": "A client-side web app using HTML, JavaScript and CSS, built with Vue.js 2. This web app displays photos from Flickr, based on location data pulled from the Google Maps API.",
    "author": {
        "name": "Ralwinn Johnson",
        "email": "relwinnjohnson@gmail.com"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "bootstrap": "^4.1.3",
        "register-service-worker": "^1.0.0",
        "v-wow": "^1.2.1",
        "vue": "^2.5.17",
        "vue-router": "^3.0.1",
        "vue2-google-maps": "^0.10.2",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.1",
        "@vue/cli-plugin-eslint": "^3.0.1",
        "@vue/cli-plugin-pwa": "^3.0.1",
        "@vue/cli-service": "^3.0.1",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "globals": {
            "google": true
        },
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}